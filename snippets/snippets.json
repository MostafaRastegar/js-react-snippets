{
  "Print to console": {
    "prefix": "lg",
    "body": [
      "console.log('===== start $1 =======>');",
      "console.log($1);",
      "console.log('<==== end   =======');"
    ],
    "description": "Log output to console"
  },

  "Import react": {
    "prefix": "impr",
    "body": ["import React from 'react';"],
    "description": "import react"
  },

  "Import propTypes": {
    "prefix": "impprop",
    "body": ["import PropTypes from 'prop-types';"],
    "description": "import propTypes"
  },

  "Add useEffect": {
    "prefix": "ueffect",
    "body": ["useEffect(() => {$1},[$2])"],
    "description": "add useEffect"
  },

  "Add propTypes": {
    "prefix": "addprop",
    "body": ["$1.propTypes = {", "$2: Propstypes.$3", "}"],
    "description": "Add propTypes"
  },

  "Add Map": {
    "prefix": "addmap",
    "body": ["$1.map( $2 => $3)"],
    "description": "add useEffect"
  },

  "Import ClassNames": {
    "prefix": "impnames",
    "body": ["import classNames from 'classnames'"],
    "description": "add classNames"
  },

  "Import Global message": {
    "prefix": "impglobal",
    "body": ["import GLOBALS from '@frontend-template/constants/globals';"],
    "description": "import GLOBAL from constants"
  },

  "Import Carousel from Common": {
    "prefix": "impCommonCarousel",
    "body": ["import Carousel from 'components/Common/Carousel';"],
    "description": "import Carousel from 'components/Common/Carousel'"
  },

  "add ClassNames": {
    "prefix": "addnames",
